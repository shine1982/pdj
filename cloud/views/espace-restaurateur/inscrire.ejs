
<div class="panel panel-default">
    <div class="panel-heading">
        <h3 class="panel-title">Votre identifiant</h3>
    </div>
    <div class="panel-body">
        <form role="form">
            <div class="form-group">
                <label for="emailRestaurateur">Email *</label>
                <input type="email" class="form-control" id="emailRestaurateur" placeholder="xxx@yyy.zzz">
            </div>
            <div class="form-group">
                <label for="exampleInputEmail1">Confirmation d'email *</label>
                <input type="email" class="form-control" id="confirmationEmailRestaurateur" placeholder="xxx@yyy.zzz">
            </div>
            <div class="form-group">
                <label for="exampleInputPassword1">Password</label>
                <input type="password" class="form-control" id="exampleInputPassword1" placeholder="Password">
            </div>

            <button type="submit" class="btn btn-default">Submit</button>
        </form>
    </div>
</div>


<%- contentFor('pageTitle') %>
Restaurateur